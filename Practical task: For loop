using System;

public class BinLaden {
    public static void Main(string[] args) {
        string[] users = ["Boneng21", "EmberTaijutsu67", "Sungaboyhipak51", "Ivano5"];
        string[] pass = ["apokongpogi", "Jessicasoju", "Mabangisperomalangis", "aldubforever"];

        Console.Write("Username : ");
        string username = Console.ReadLine() ?? "";

        Console.Write("Password : ");
        string password = Console.ReadLine() ?? "";

        bool matched = false;

        for (int i = 0; i < users.Length; i++) {
            if (username.Equals(users[i]) && password.Equals(pass[i])) {
                matched = true;
                break;
            }
        }
        if (matched) {
            Console.WriteLine("Welcome, User.");
        }
        else {
            Console.WriteLine("Username or password is incorrect, Please try again.");
        }
    }
}
